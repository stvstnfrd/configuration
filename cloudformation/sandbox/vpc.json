{
    "AWSTemplateFormatVersion" : "2010-09-09",
    "Description": "Sandbox Setup",
    "Parameters": {
    },
    "Mappings": {
        "SubnetConfig": {
            "vpc": {
                "cidr": "10.4.0.0/16"
            }
        }
    },
    "Resources": {
        "vpc": {
            "Type" : "AWS::EC2::VPC",
            "Properties" : {
                "CidrBlock" : { "Fn::FindInMap" : [ "SubnetConfig", "vpc", "cidr" ] },
                "Tags" : [
                    {"Key" : "Name", "Value" : "vpc-sandbox"},
                    {"Key" : "Application", "Value" : { "Ref" : "AWS::StackId"} }
                ]
            }
        }
    },
    "Outputs": {
    }
}
